{
	"mappings":
	{
		"required": ["fields"]
	}
,	"transforms":
	{
		"required": ["storage"]
	,	"optional": ["associations","collection","root"] 
	}
,	"associations":
	{
		"has-many":
		{
			"required": ["type","target","target_key"]
		,	"optional": ["key","embedded","collection"] 
		}
	,	"has-one":
		{
			"required": ["type","target","target_key"]
		,	"optional": ["key","embedded"] 
		}
	,	"has-many:through":
		{
			"required": ["type","target","through"]
		,	"optional": ["key","target_key","through_key","through_target_key","embedded","collection"] 
		}
	,	"has-one:through":
		{
			"required": ["type","target","through"]
		,	"optional": ["key","target_key","through_key","through_target_key","embedded"] 
		}
	,	"belongs-to":
		{
			"required": ["type","target","key"]
		,	"optional": ["target_key","embedded"] 
		}
	,	"has-and-belongs-to-many":
		{
			"required": ["type","target","through_target_key","through"]
		,	"optional": ["through_key","key","target_key", "embedded", "collection"] 
		}
	,	"link":
		{
			"required": ["type","template"]
		,	"optional": ["attrs","embedded"]
		}
	,	"is-a":
		{
			"required": ["type","target","key"]
		,	"optional": ["target_key"]
		}
	}
,	"embedded":
	{
		"type": ["none","partial","full","*"]
	,	"optional": ["collection"]
	}
,	"collection":
	{
		"required": ["type"]
	,	"optional": ["ipp","page"]
	}
}